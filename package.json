{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "Minimal web boilerplate",
  "main": "./src/index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack serve --port 2333 --mode development --open",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "lint": "stylelint ./src/**/*.scss ./src/**/*.css && eslint --ext .js --ext .jsx ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/boilerplate.git"
  },
  "engines": {
    "node": ">=9"
  },
  "keywords": [
    "webpack",
    "boilerplate"
  ],
  "author": "sabertazimi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sabertazimi/boilerplate/issues"
  },
  "homepage": "https://github.com/sabertazimi/boilerplate",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "autoprefixer": "^10.3.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.6.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.2",
    "image-webpack-loader": "^7.0.1",
    "mini-css-extract-plugin": "^1.6.2",
    "node-sass": "^6.0.0",
    "postcss-loader": "^4.3.0",
    "sass-loader": "^10.2.0",
    "style-loader": "^2.0.0",
    "stylelint": "^9.10.1",
    "stylelint-config-mass": "^1.0.2",
    "stylelint-webpack-plugin": "^0.10.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
