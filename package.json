{
  "name": "boilerplate",
  "version": "2.10.0",
  "description": "Minimal web boilerplate",
  "keywords": [
    "webpack",
    "boilerplate"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/boilerplate.git"
  },
  "author": "sabertazimi",
  "main": "./src/index.js",
  "engines": {
    "node": ">=14"
  },
  "bugs": {
    "url": "https://github.com/sabertazimi/boilerplate/issues"
  },
  "homepage": "https://github.com/sabertazimi/boilerplate",
  "packageManager": "yarn@3.6.4",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "badge": "ts-node scripts/badge.ts",
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "cross-env NODE_ENV=development webpack serve",
    "lint": "yarn lint:style && yarn lint:type-check",
    "lint:style": "stylelint ./src/**/*.css && eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:type-check": "tsc --noEmit",
    "changeset": "commit-and-tag-version --dry-run -s",
    "release": "commit-and-tag-version -s",
    "start": "yarn dev",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.23.2",
    "@dg-scripts/eslint-config": "^5.14.0",
    "@dg-scripts/stylelint-config": "^5.14.0",
    "@svgr/webpack": "^8.1.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^18.18.6",
    "babel-loader": "^9.1.3",
    "commit-and-tag-version": "^11.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.2.0",
    "prettier": "^3.0.3",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "stylelint": "^15.11.0",
    "stylelint-webpack-plugin": "^4.1.1",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "undici": "^5.26.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpackbar": "^5.0.2"
  }
}
