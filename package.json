{
  "name": "boilerplate",
  "version": "2.10.0",
  "description": "Minimal web boilerplate",
  "keywords": [
    "webpack",
    "boilerplate"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/boilerplate.git"
  },
  "author": "sabertazimi",
  "main": "./src/index.js",
  "engines": {
    "node": ">=14"
  },
  "bugs": {
    "url": "https://github.com/sabertazimi/boilerplate/issues"
  },
  "homepage": "https://github.com/sabertazimi/boilerplate",
  "packageManager": "yarn@3.2.1",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "badge": "ts-node scripts/badge.ts",
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "cross-env NODE_ENV=development webpack serve",
    "lint": "yarn lint:style && yarn lint:type-check",
    "lint:style": "stylelint ./src/**/*.css && eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:type-check": "tsc --noEmit",
    "changeset": "standard-version --dry-run -s",
    "release": "standard-version -s",
    "start": "yarn dev",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/preset-env": "^7.18.2",
    "@dg-scripts/eslint-config": "^5.10.0",
    "@dg-scripts/stylelint-config": "^5.10.0",
    "@svgr/webpack": "^6.2.1",
    "@types/jest": "^28.1.0",
    "@types/node": "^16.11.38",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.17.0",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.14",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^7.0.0",
    "postcss-preset-env": "^7.7.1",
    "prettier": "^2.6.2",
    "sass-loader": "^13.0.0",
    "standard-version": "^9.5.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.8.5",
    "stylelint-webpack-plugin": "^3.3.0",
    "ts-jest": "^28.0.4",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3",
    "undici": "^5.4.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.1",
    "webpackbar": "^5.0.2"
  }
}
